{
  "name": "{{ cookiecutter.repo_name }}",
  "version": "1.0.0",
  "private": true,
  "prettier": "@thorgate/prettier-config",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/react": "5.20.1",
    "@thorgate/babel-preset": "^2.0.0",
    "autoprefixer": "8.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "8.2.2",
    "babel-preset-razzle": "^4.0.4",
    "bootstrap": "4.3.1",
    "css-loader": "1.0.0",
    {%- if cookiecutter.use_cypress == YES %}
    "cypress": "^7.1.0",
    "cypress-multi-reporters": "^1.4.0",
    {%- endif %}
    "extract-css-chunks-webpack-plugin": "3.1.1",
    "file-loader": "1.1.11",
    "node-sass": "6.0.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "2.1.5",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "16.14.0",
    "react-redux": "7.2.2",
    "redux-logger": "^3.0.6",
    "resolve-url-loader": "2.3.0",
    "sass-loader": "7.1.0",
    "url-loader": "1.0.1",
    "webpack": "4.17.2",
    "webpack-bundle-tracker": "0.4.0-beta",
    "webpack-cli": "3.1.0"
  },
  "devDependencies": {
    {%- if cookiecutter.webapp_include_storybook == YES %}
    "@storybook/addon-storyshots": "^6.1.21",
    "@storybook/react": "^6.1.21",
    {%- endif %}
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@thorgate/eslint-config": "^2.1.0",
    "@thorgate/eslint-config-typescript": "^2.1.0",
    "@thorgate/prettier-config": "1.0.1",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "2.2.1",
    {%- if cookiecutter.webapp_include_storybook == YES %}
    "react-test-renderer": "16.14.0",
    {%- endif %}
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "webapp/src/**/*.{js,jsx}",
      "!webapp/src/main.js",
      "!webapp/src/test-setup.js",
      "!webapp/src/test-utils.js",
      "!webapp/src/**/*.stories.js",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "cobertura"
    ],
    "globals": {
      "NODE_ENV": "test",
      "DJ_CONST": {}
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/webapp/src",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "styles-src": "<rootDir>/webapp/src/stylesStub.js",
      "\\.(css|less|scss)$": "<rootDir>/webapp/src/stylesStub.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "<rootDir>/webapp/src/test-setup.js"
    ],
    {%- if cookiecutter.use_cypress == YES %}
    "testPathIgnorePatterns": [
      "<rootDir>/cypress/"
    ],
    {%- endif %}
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "scripts": {
    "dev": "cd webapp && NODE_ENV=development webpack --config webpack/config.dev.js --watch --progress --profile --color",
    "build": "cd webapp && NODE_ENV=production webpack -p --config webpack/config.production.js --progress --profile --color",
    "build-quiet": "cd webapp && NODE_ENV=production webpack -p --config webpack/config.production.js",
    {%- if cookiecutter.webapp_include_storybook == YES %}
    "build-storybook": "build-storybook -c webapp/src/.storybook",
    {%- endif %}
    "clean": "cd webapp && rm -rf build/",
    {%- if cookiecutter.use_cypress == YES %}
    "cy:run-ci": "cypress run --browser chrome --headless",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    {%- endif %}
    "prebuild": "yarn clean",
    "prettier-check": "prettier --check",
    "prettier-check-all": "yarn prettier-check 'webapp/src/**/*.{js,jsx,ts,tsx}'",
    "prettier-format": "prettier --write",
    "prettier-format-all": "yarn prettier-format 'webapp/src/**/*.{js,jsx,ts,tsx}'",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint webapp/src",
    "lint:tsc": "tsc --noEmit",
    {%- if cookiecutter.webapp_include_storybook == YES %}
    "storybook": "start-storybook -c webapp/src/.storybook -p 6006",
    {%- endif %}
    "stylelint": "cd static/styles-src && stylelint '**/*.scss'",
    "test": "NODE_ENV=test jest",
    {%- if cookiecutter.webapp_include_storybook == YES %}
    "test:update": "NODE_ENV=test jest -u",
    {%- endif %}
    "test:watch": "NODE_ENV=test jest --watch"
  }
}
