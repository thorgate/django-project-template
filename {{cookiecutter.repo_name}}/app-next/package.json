{
  "name": "{{cookiecutter.project_title}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-env": "node ./scripts/generate-env.js",
    "dev": "pnpm generate-env && next dev",
    "build": "next build",
    "start": "pnpm generate-env && next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:eslint": "next lint --max-warnings 0",
    "lint:tsc": "tsc --noEmit",
    "lint": "pnpm lint:eslint && pnpm lint:tsc",
    "prettier-check": "prettier --check",
    "prettier-check-all": "prettier --check '{lib,src,app,__tests__,components,pages,scripts,types}/**/*.{js,jsx,ts,tsx}'",
    "prettier-format": "prettier --write",
    "prettier-format-all": "prettier --write '{lib,src,app,__tests__,components,pages,scripts,types}/**/*.{js,jsx,ts,tsx}'",
    {%- if cookiecutter.use_cypress == YES %}
    "cy:run-ci": "wait-on http://node-cypress.TODO.com.docker.local:9990 && cypress run --browser chrome --headless",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    {%- endif %}
    "generate-api": "pnpm rtk-query-codegen-openapi ./lib/queries/openapiConfig.ts && pnpm prettier-format-all",
    "generate-api-mock": "pnpm msw-auto-mock lib/queries/schema.json -o mock/api/handlers.generated.js && pnpm prettier-format-all",
    "extract-i18n": "i18next-scanner --config ./scanner.config.js",
    "postextract-i18n": "pnpm i18next-resources",
    "i18next-resources": "i18next-resources-for-ts interface -i ./public/locales/en -o ./types/i18next-resources.d.ts && prettier --write ./types/i18next-resources.d.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.17",
    "@reduxjs/toolkit": "^1.9.3",
    "@rtk-query/codegen-openapi": "1.2.0",
    "@sentry/nextjs": "^7.47.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "i18next": "^22.4.14",
    "i18next-chained-backend": "^4.2.0",
    "i18next-http-backend": "^2.2.0",
    "i18next-localstorage-backend": "^4.1.1",
    "i18next-resources-for-ts": "^1.5.0",
    "i18next-scanner": "^4.2.0",
    "jose": "^4.14.0",
    "lodash.throttle": "^4.1.1",
    "lorem-ipsum": "^2.0.8",
    "minimist": "^1.2.0",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "next-i18next": "^13.2.2",
    "next-redux-wrapper": "9.0.0-rc.2",
    "numeral": "^2.0.6",
    "openapi-types": "12.1.0",
    "qs": "^6.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.8",
    "react-i18next": "^12.2.0",
    "react-number-format": "^5.2.2",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "18.15.10",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "@types/redux-logger": "^3.0.9",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.9.0",
    "cypress-multi-reporters": "^1.6.3",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^15.0.0",
    "mocha": "^10.2.0",
    "msw": "^1.2.2",
    "msw-auto-mock": "^0.15.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "ts-node": "10.9.1",
    "wait-on": "^7.0.1"
  }
}
