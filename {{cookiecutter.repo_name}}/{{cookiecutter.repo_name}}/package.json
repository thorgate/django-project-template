  "scripts": {
    "dev": "cd app && webpack --config webpack/config.dev.js --watch --progress --profile --color",
    "build": "cd app && webpack -p --config webpack/config.production.js --progress --profile --color",
    "build-quiet": "cd app && webpack -p --config webpack/config.production.js",
    "clean": "cd app && rm -rf build/",
    "prebuild": "npm run clean",
    "lint": "cd app/src && eslint .",
    "stylelint": "cd static/styles-src && stylelint '**/*.scss'",
