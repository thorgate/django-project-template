{
  "name": "app-new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-env": "node ./scripts/generate-env.js",
    "dev": "pnpm generate-env && next dev",
    {%- if cookiecutter.use_cypress == YES %}
    "cy:run-ci": "wait-on http://node-cypress.{{cookiecutter.live_domain_name}}.docker.local:9990 && cypress run --browser chrome --headless",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open",
    {%- endif %}
    "build": "next build",
    "start": "pnpm generate-env && next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "pnpm lint:eslint && pnpm lint:tsc",
    "lint:eslint": "next lint",
    "lint:tsc": "tsc --noEmit",
    "prettier-check": "prettier --check",
    "prettier-check-all": "prettier --check '{lib,src}/**/*.{js,jsx,ts,tsx}'",
    "prettier-format": "prettier --write",
    "prettier-format-all": "prettier --write '{lib,src}/**/*.{js,jsx,ts,tsx}'",
    "generate-api": "pnpm rtk-query-codegen-openapi ./lib/queries/openapiConfig.ts && pnpm prettier-format-all",
    "extract-i18n": "i18next-scanner --config ./scanner.config.js"
  },
